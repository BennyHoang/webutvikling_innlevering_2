@model IEnumerable<NyheterApp.Models.Nyhet>

@{
    ViewBag.Title = "VisEnNyhet";
}

<h2>Vis En Nyhet</h2>
@if (Model != null && Model.Count() > 0)
{
    foreach (var Nyhets in Model)
    {
        <article>
            <h3>@Nyhets.Tittel</h3>
        </article>
    }
}
else
{
    <p>no articles</p>
}


    
